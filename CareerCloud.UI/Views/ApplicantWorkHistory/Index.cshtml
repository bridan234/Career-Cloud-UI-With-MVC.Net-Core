@model IEnumerable<CareerCloud.Pocos.ApplicantWorkHistoryPoco>

@{
    ViewData["Title"] = "Work Experience";
    var Name = "";
    Name = Model.Count() <= 0 ? " " : Model.FirstOrDefault().ApplicantProfile.SecurityLogin.FullName;
}

<h1>@Name</h1>


@await Html.PartialAsync("~/Views/Shared/Partial_Views/_ApplicantWorkHistory.cshtml", new ViewDataDictionary(ViewData) { { "Applicant", ViewData["Id"] } })
<div>
    <a class="btn-block" asp-action="Index" asp-controller="applicantprofile">Back To List</a>
</div>